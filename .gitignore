/app/javascript/translations/translations.js

# Ignore folder information and IDE-specific files
.DS_Store
.idea/*

# Ignore the test coverage results from SimpleCov
/coverage

# Ignore pronto configuration files
.pronto.yml
file_uploads/
dump.rdb